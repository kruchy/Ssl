	$(MOC) sender.h -o moc_sender.cpp

####### Install

install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/network/broadcastsender/" || $(MKDIR) "$(INSTALL_ROOT)/network/broadcastsender/"
	-$(INSTALL_FILE) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/network/broadcastsender/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/network/broadcastsender/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/network/broadcastsender/"


install_sources: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/network/broadcastsender/" || $(MKDIR) "$(INSTALL_ROOT)/network/broadcastsender/"
	-$(INSTALL_FILE) "sender.cpp" "$(INSTALL_ROOT)/network/broadcastsender/"
	-$(INSTALL_FILE) "main.cpp" "$(INSTALL_ROOT)/network/broadcastsender/"
	-$(INSTALL_FILE) "sender.h" "$(INSTALL_ROOT)/network/broadcastsender/"


uninstall_sources: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/network/broadcastsender/sender.cpp"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/network/broadcastsender/main.cpp"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/network/broadcastsender/sender.h"
	-$(DEL_DIR) "$(INSTALL_ROOT)/network/broadcastsender/"


install: install_target install_sources  

uninstall: uninstall_target uninstall_sources  

